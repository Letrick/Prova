            if(numero4 > numero2){
                cout<<numero4<<", "<<numero2;
            }
        }
        if(numero2 > numero1 &&numero2 > numero4){
            cout<<numero2<<", ";
            if(numero1 > numero4){
                cout<<numero1<<", "<<numero4;
            }
            if(numero4 > numero1){
                cout<<numero4<<", "<<numero1;
            }
        } 
        if(numero4 > numero1 &&numero4 > numero2){
            cout<<numero4<<", ";
            if(numero1 > numero2){
                cout<<numero1<<", "<<numero2;
            }
            if(numero2 > numero1){
                cout<<numero2<<", "<<numero1;
            }
        }    
    }
    if(numero4 > numero1 && numero4 > numero2 && numero4 > numero3){
        cout<<numero4<<", ";
        if(numero1 > numero2 &&numero1 > numero3){
            cout<<numero1    <<", ";
            if(numero2 > numero3){
                cout<<numero2<<", "<<numero3;
            }
            if(numero3 > numero2){
                cout<<numero3<<", "<<numero2;
            }
        }
        if(numero2 > numero1 &&numero2 > numero3){
            cout<<numero2<<", ";
            if(numero1 > numero3){
                cout<<numero1<<", "<<numero3;
            }
            if(numero3 > numero1){
                cout<<numero3<<", "<<numero1;
