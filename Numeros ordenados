        if(numero2 > numero3 &&numero2 > numero4){
            cout<<numero2<<", ";
            if(numero3 > numero4){
                cout<<numero3<<", "<<numero4;
            }
            if(numero4 > numero3){
                cout<<numero4<<", "<<numero3;
